{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Draggable": true,
    "TimelineLite": true,
    "TimelineMax": true,
    "TweenLite": true,
    "TweenMax": true
  },
  "rules": {
    "no-console": ["error", { allow: ["warn", "error"] }],
    "no-unused-vars": 1
  },
  "parserOptions": {
    "sourceType": "module"
  }
}