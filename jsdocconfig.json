{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ 
            "./src/scripts/animation.js",
            "./src/scripts/camera.js",
            "./src/scripts/scene.js"
        ],
        "exlude": [ 
            "./src/scripts/lib"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "readme": "README.md"
    }
}
